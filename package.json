{
  "name": "@parkhub/circe-middleware",
  "author": "Daniel Olivares",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "start": "nps",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "opt --in pre-commit --exec \"npm start validate\""
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=3"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@parkhub/eslint-config-parkhub-base": "^1.1.0",
    "are-we-flow-yet": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "codecov": "^2.3.0",
    "commitizen": "^2.9.6",
    "condition-codefresh": "^1.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "documentation": "^5.3.1",
    "eslint": "^4.6.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.0.2",
    "flow-bin": "^0.54.1",
    "flow-copy-source": "^1.2.1",
    "flow-remove-types": "^1.2.1",
    "flow-typed": "^2.1.5",
    "husky": "^0.14.3",
    "jest": "^21.0.2",
    "leasot": "^4.10.1",
    "nps": "^5.7.1",
    "nps-utils": "^1.3.0",
    "opt-cli": "^1.5.2",
    "prettier-eslint-cli": "4.3.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^7.0.2",
    "validate-commit-msg": "^2.14.0"
  },
  "release": {
    "verifyConditions": "condition-codefresh",
    "debug": false
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testMatch": [
      "**/src/**/?(*.)test.js?(x)"
    ],
    "modulePathIgnorePatterns": [
      "dist/*"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parkhub/circe-middleware.git"
  },
  "bugs": {
    "url": "https://github.com/parkhub/circe-middleware/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
